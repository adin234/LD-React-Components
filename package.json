    "react-test-renderer": "^16.13.1",
    "sinon": "^9.1.0",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@storybook/addon-knobs": "^6.0.22",
    "hash.js": "~1.1.7",
    "ldclient-js": "^2.10.2",
    "prop-types": "^15.7.2",
    "snyk": "^1.406.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "snyk": true
}

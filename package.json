{
  "name": "ld-react-components",
  "version": "1.0.78",
  "description": "Semantic component helpers to support LaunchDarkly in your react app.",
  "main": "dist/index.js",
  "author": {
    "name": "Dave Bergschneider",
    "email": "dave@cmscode.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intuit/LD-React-Components"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "scripts": {
    "start": "start-storybook -p 6010",
    "prepare": "npm run snyk-protect && yarn build",
    "clean": "rimraf ./dist ./coverage ./docs ./test-results",
    "test": "yarn add --dev jest-junit && yarn build && jest --coverage --reporters=default --reporters=jest-junit && mkdir -p test-reports/jest && mv junit.xml test-reports/jest/results.xml",
    "lint": "eslint src/",
    "build": "babel src/lib -d dist --copy-files && tsc",
    "build:watch": "babel src/lib -w -d lib --copy-files",
    "docs:build": "build-storybook -o docs",
    "docs:config": "mkdirp ./docs/.circleci/ && cp .storybook/config.yml docs/.circleci/ && cp -R .storybook/old-docs docs/",
    "docs:publish": "push-dir --cleanup --dir=docs --branch=gh-pages --force",
    "release": "auto shipit",
    "snyk-protect": "snyk protect"
  },
  "keywords": [
    "launch-darkly",
    "feature",
    "flags",
    "react",
    "open source"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-a11y": "^6.0.22",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-docs": "^6.0.22",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-storysource": "^6.0.22",
    "@storybook/addons": "^6.0.22",
    "@storybook/react": "^6.0.22",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/storybook__react": "^4.0.2",
    "all-contributors-cli": "^6.17.4",
    "auto": "^9.53.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "css-loader": "^4.3.0",
    "enzyme": "~3.10.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "~14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-standard": "~4.0.1",
    "expect": "^26.4.2",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "~26.0.1",
    "jest-junit": "^11.1.0",
    "jsdom": "16.4.0",
